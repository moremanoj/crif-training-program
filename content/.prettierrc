    "useTabs": false,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "endOfLine": "lf",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "svelteBracketNewLine": true,
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        }
    ]
